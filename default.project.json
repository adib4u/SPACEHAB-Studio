{
  "name": "GAME",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      },
      "Configs": {
        "$path": "src/configs"
      }
    },

    "ServerScriptService": {
      "$path": "src/server"
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "$path": "src/client"
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}